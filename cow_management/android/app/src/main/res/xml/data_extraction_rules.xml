<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 클라우드 백업에서 제외할 데이터 -->
        <exclude domain="sharedpref" path="flutter_secure_storage" />
        <exclude domain="database" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- 기기 간 전송에서 제외할 데이터 -->
        <exclude domain="sharedpref" path="flutter_secure_storage" />
        <exclude domain="database" path="." />
    </device-transfer>
</data-extraction-rules>